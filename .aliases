# shell
alias szsh="source ~/.zshrc"

# vim
alias v="nvim"
alias vim="nvim"

# dot files
alias dots='/usr/bin/git --git-dir=$HOME/.dotfiles/ --work-tree=$HOME'
alias dots_qs="dots add -u && dots commit -m 'hi' && dots push"

# terraform
alias tf="terraform"

# chrome
alias chrome="/Applications/Google\ Chrome.app/Contents/MacOS/Google\ Chrome" 

source $HOME/scripts/git_aliases.sh
source $HOME/scripts/uv_aliases.sh
source $HOME/scripts/python_aliases.sh

# venv
alias qvenv="source .venv/bin/activate"
alias pip="uv pip"

# runpod
alias rp="runpodctl"
